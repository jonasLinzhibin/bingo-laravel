{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "akaunting/setting": "^1.0",
        "caouecs/laravel-lang": "~3.0",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelnews/laravel-twbs4": "^1.3",
        "maennchen/zipstream-php": "^0.5.2",
        "mews/purifier": "^2.1",
        "peinhu/aetherupload-laravel": "~1.0",
        "predis/predis": "^1.1",
        "spatie/laravel-image-optimizer": "^1.3",
        "spatie/laravel-medialibrary": "6.0.0",
        "spatie/laravel-menu": "^3.2",
        "spatie/laravel-missing-page-redirector": "^2.3",
        "spatie/laravel-permission": "^2.27",
        "spatie/temporary-directory": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.5",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "sven/artisan-view": "^2.1",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Helpers/Util"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/Function/functions.php"
        ]

    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
